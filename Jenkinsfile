pipeline {
    agent any

    stages {

        stage('Checkout Code') {
            steps {
                echo "Checking out source code from GitHub"
            }
        }

        stage('Build Simulation') {
            steps {
                echo "Simulating build process"
                echo "VSAM modernization pipeline executed successfully"
            }
        }

        stage('Finish') {
            steps {
                echo "CI pipeline completed"
            }
        }
    }
}
